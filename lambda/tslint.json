{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": false,
    "arrow-parens": [true, "ban-single-arg-parens"],
    "object-literal-sort-keys": false,
    "radix": false,
    "no-console": false,
    "max-line-length": [
      true,
      {
        "ignore-pattern": "@|://"
      }
    ]
  },
  "rulesDirectory": []
}
